<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="me.henrytao.smoothappbarlayoutdemo">

  <uses-permission android:name="com.android.vending.BILLING" />

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:theme="@style/AppTheme">
    <activity
      android:label="@string/app_name"
      android:name=".activity.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:label="@string/title_activity_info"
      android:name=".activity.InfoActivity" />
    <activity
      android:label="@string/title_activity_feature"
      android:name=".activity.FeatureActivity" />
    <activity
      android:label="@string/title_activity_inbox"
      android:name=".activity.InboxActivity" />
    <activity
      android:label="@string/title_activity_google_play"
      android:name=".activity.GooglePlayActivity" />
    <activity
      android:label="@string/title_activity_smooth_view_pager_drawer_layout"
      android:name=".activity.SmoothViewPagerDrawerLayoutActivity"></activity>
    <activity
      android:label="@string/title_activity_smooth_view_pager_parallax_drawer_layout"
      android:name=".activity.SmoothViewPagerParallaxDrawerLayoutActivity"></activity>
  </application>

</manifest>
